{
  "name": "NagaPortalAgent",
  "displayName": "娜迦官网API Agent",
  "version": "0.1.0",
  "description": "与娜迦官网API进行交互的MCP服务（充值功能）",
  "author": "NagaAgent",
  "agentType": "mcp",
  "entryPoint": {
    "module": "mcpserver.agent_naga_portal.naga_portal_agent",
    "class": "NagaPortalAgent"
  },
  "factory": {
    "create_instance": "create_naga_portal_agent",
    "validate_config": "validate_agent_config",
    "get_dependencies": "get_agent_dependencies"
  },
  "communication": {
    "protocol": "stdio",
    "timeout": 15000
  },
  "capabilities": {
    "invocationCommands": [
      {
        "command": "naga_recharge",
        "description": "额度充值；参数：amount（必填，充值金额）、payment_type（可选，支付方式，默认wxpay，支持wxpay/alipay）",
        "example": "{\"tool_name\":\"naga_recharge\",\"amount\":\"11.00\",\"payment_type\":\"wxpay\"}"
      },
      {
        "command": "naga_redeem_code",
        "description": "兑换码使用；参数：key（必填，兑换码）",
        "example": "{\"tool_name\":\"naga_redeem_code\",\"key\":\"aaaa\"}"
      },
      {
        "command": "naga_balance",
        "description": "余额查询；无需参数",
        "example": "{\"tool_name\":\"naga_balance\"}"
      },
      {
        "command": "naga_apply_token",
        "description": "智能申请API令牌；参数：name（可选，令牌名称）、group（可选，模型组）、unlimited_quota（可选，是否无限制额度，默认true）。如果不提供参数，会返回可用模型列表供选择",
        "example": "{\"tool_name\":\"naga_apply_token\"}"
      },
      {
        "command": "naga_get_tokens",
        "description": "获取已配置的API令牌列表；无需参数",
        "example": "{\"tool_name\":\"naga_get_tokens\"}"
      }
    ]
  },
  "inputSchema": {
    "type": "object",
    "properties": {
      "tool_name": {"type": "string"},
      "amount": {"type": "string"},
      "payment_type": {"type": "string"},
      "key": {"type": "string"},
      "name": {"type": "string"},
      "group": {"type": "string"},
      "unlimited_quota": {"type": "boolean"}
    },
    "required": ["tool_name"]
  },
  "outputSchema": {
    "type": "object",
    "properties": {
      "success": {"type": "boolean"},
      "status": {"type": "string"},
      "message": {"type": "string"},
      "data": {"type": "object"},
      "error": {"type": "string"}
    }
  },
  "runtime": {
    "instance": null,
    "is_initialized": false,
    "last_used": null,
    "usage_count": 0
  }
}


